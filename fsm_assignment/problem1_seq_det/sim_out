v0x5ac46a0b2f00_0 .var "y", 0 0;
E_0x5ac46a096cd0 .event posedge, v0x5ac46a05d520_0;
E_0x5ac46a05dba0 .event anyedge, v0x5ac46a0b2df0_0, v0x5ac46a0b2d10_0;
    .scope S_0x5ac46a09afb0;
T_0 ;
    %wait E_0x5ac46a05dba0;
    %load/vec4 v0x5ac46a0b2df0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5ac46a0b2b60_0, 0, 2;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5ac46a0b2d10_0;
    %assign/vec4 v0x5ac46a0b2b60_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5ac46a09afb0;
T_1 ;
    %wait E_0x5ac46a096cd0;
    %load/vec4 v0x5ac46a0b2b60_0;
    %store/vec4 v0x5ac46a0b2d10_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ac46a0b2f00_0, 0, 1;
    %load/vec4 v0x5ac46a0b2b60_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5ac46a0b2d10_0, 0, 2;
    %jmp T_1.5;
T_1.0 ;
    %load/vec4 v0x5ac46a0b2c40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.6, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5ac46a0b2d10_0, 0, 2;
    %jmp T_1.7;
T_1.6 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5ac46a0b2d10_0, 0, 2;
T_1.7 ;
    %jmp T_1.5;
T_1.1 ;
