    %jmp T_1.9;
T_1.8 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5ac46a0b2d10_0, 0, 2;
T_1.9 ;
    %jmp T_1.5;
T_1.2 ;
    %load/vec4 v0x5ac46a0b2c40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.10, 8;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5ac46a0b2d10_0, 0, 2;
    %jmp T_1.11;
T_1.10 ;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x5ac46a0b2d10_0, 0, 2;
T_1.11 ;
    %jmp T_1.5;
T_1.3 ;
    %load/vec4 v0x5ac46a0b2c40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.12, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5ac46a0b2f00_0, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5ac46a0b2d10_0, 0, 2;
    %jmp T_1.13;
T_1.12 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5ac46a0b2f00_0, 0;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5ac46a0b2d10_0, 0, 2;
T_1.13 ;
    %jmp T_1.5;
T_1.5 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5ac46a09ae20;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ac46a0b3040_0, 0, 1;
    %end;
